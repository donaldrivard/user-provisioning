<aura:component>
    
    <!-- #Data -->
  	<!-- NOTE: I am using a two-way binding for customer data between the browser, search, and detail components.
       When the search component selects a customer it is synchronized between the browser and and therefore the detail. -->
  	<aura:attribute name="selectedAccountId" type="String"/>
  	<aura:attribute name="selectedAccount" type="Object"/>

  <div class="slds-grid slds-wrap slds-grid--pull-padded">

    <div class="slds-col--padded-large slds-size--4-of-6">
      <c:CC_AccountSearch selectedAccountId="{!v.selectedAccountId}"/>
    </div>
    
  </div>
    
    
</aura:component>	
